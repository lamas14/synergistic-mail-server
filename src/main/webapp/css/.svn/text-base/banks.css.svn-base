#go_button {
	width: auto;
	background: #2f99ca;
	border: 1px solid #1e728c;
	-webkit-box-shadow: 0 1px 0 rgba(255, 255, 255, 0.5) inset;
	-moz-box-shadow: 0 1px 0 rgba(255, 255, 255, 0.5) inset;
	box-shadow: 0 1px 0 rgba(255, 255, 255, 0.5) inset;
	color: white;
	cursor: pointer;
	display: inline-block;
	font-family: inherit;
	font-size: 14px;
	font-weight: bold;
	line-height: 1;
	margin: 0;
	padding: 2px 2px 2px;
	text-align: center;
	text-decoration: none;
	-webkit-transition: background-color 0.15s ease-in-out;
	-moz-transition: background-color 0.15s ease-in-out;
	-o-transition: background-color 0.15s ease-in-out;
}

#go_button:hover,.go_button:focus {
	color: white;
	background-color: #D81919;
}

#go_button:active {
	-webkit-box-shadow: 0 1px 0 rgba(0, 0, 0, 0.2) inset;
	-moz-box-shadow: 0 1px 0 rgba(0, 0, 0, 0.2) inset;
	box-shadow: 0 1px 0 rgba(0, 0, 0, 0.2) inset;
}

#go_button.large {
	font-size: 17px;
	padding: 15px 30px 16px;
}

#go_button.medium {
	font-size: 14px;
}

#go_button.small {
	font-size: 11px;
	padding: 7px 14px 8px;
}

#go_button.tiny {
	font-size: 10px;
	padding: 5px 10px 6px;
}

#go_button.expand {
	width: 100%;
	text-align: center;
}

#go_button.primary {
	background-color: #2ba6cb;
	border: 1px solid #1e728c;
}

#go_button.primary:hover,.go_button.primary:focus {
	background-color: #2284a1;
}

#go_button.success {
	background-color: #5da423;
	border: 1px solid #396516;
}

#go_button.success:hover,.go_button.success:focus {
	background-color: #457a1a;
}

#go_button.alert {
	background-color: #c60f13;
	border: 1px solid #7f0a0c;
}

#go_button.alert:hover,.go_button.alert:focus {
	background-color: #970b0e;
}

#go_button.secondary {
	background-color: #e9e9e9;
	color: #1d1d1d;
	border: 1px solid #c3c3c3;
}

#go_button.secondary:hover,.go_button.secondary:focus {
	background-color: #d0d0d0;
}

#go_button.radius {
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	-ms-border-radius: 3px;
	-o-border-radius: 3px;
	border-radius: 3px;
}

#go_button.round {
	-webkit-border-radius: 1000px;
	-moz-border-radius: 1000px;
	-ms-border-radius: 1000px;
	-o-border-radius: 1000px;
	border-radius: 1000px;
}

#go_button.full-width {
	width: 100%;
	text-align: center;
	padding-left: 0px !important;
	padding-right: 0px !important;
}

#go_button.left-align {
	text-align: left;
	text-indent: 12px;
}

#go_button.disabled,.go_button[disabled] {
	opacity: 0.6;
	cursor: default;
	background: #2ba6cb;
	-webkit-box-shadow: none;
	-moz-box-shadow: none;
	box-shadow: none;
}

#go_button.disabled:hover,.go_button[disabled]:hover {
	background: #2ba6cb;
}

#go_button.disabled.success,.go_button[disabled].success {
	background-color: #5da423;
}

#go_button.disabled.success:hover,.go_button.disabled.success:focus,.go_button[disabled].success:hover,.go_button[disabled].success:focus
	{
	background-color: #5da423;
	outline: none;
}

#go_button.disabled.alert,.go_button[disabled].alert {
	background-color: #c60f13;
}

#go_button.disabled.alert:hover,.go_button.disabled.alert:focus,.go_button[disabled].alert:hover,.go_button[disabled].alert:focus
	{
	background-color: #c60f13;
	outline: none;
}

#go_button.disabled.secondary,.go_button[disabled].secondary {
	background-color: #e9e9e9;
}

#go_button.disabled.secondary:hover,.go_button.disabled.secondary:focus,.go_button[disabled].secondary:hover,.go_button[disabled].secondary:focus
	{
	background-color: #e9e9e9;
	outline: none;
}

