#credits {
	float: right;
	height: 100px;
	font-family: "Century Gothic";
	color: white;
	font-size: 0.7em;
	font-weight: light;
}

#fotter-content {
	width: 900px;
	margin: 0 auto;
	padding: 10px;
	height: 120px;
}

#end-fotter {
	margin: 0 auto;
	margin-top: 125px;
	font-size: 10px;
	color: #ccccc1;
}

.credits {
	color: #0099FF;
}

.fotter-box {
	margin-left: 60px;
	width: 180px;
	float: left;
	text-align: center;
}

.fotter-box2 {
	margin-left: 60px;
	width: 180px;
	float: right;
	text-align: center;
}

.fotter-box h2 {
	color: #fffff1;
}

.fotter-box a {
	color: #ccc;
	text-decoration: none;
}

.fotter-box a:hover {
	color: #0099FF;
	text-decoration: underline;
}

.fotter-box li {
	list-style: none;
}

#credits a {
	color: white;
	text-decoration: none;
	margin-left: 20px;
	float: right;
}

#credits a:hover {
	color: #00b2ee;
}

p1 {
	font-size: 1em;
}