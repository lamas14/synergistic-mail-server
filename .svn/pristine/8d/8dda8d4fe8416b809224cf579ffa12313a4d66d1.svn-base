.wrap {
	position: relative;
	margin: 0 auto;
	font-size: 1.5em;
}

.wrapper {
	min-height: 100%;
	height: auto !important;
	height: 100%;
	margin: 0 auto -4em;
}

#footer {
	width: 100%;
	float: left;
	background: url(../images/footer-bg.png) repeat-x;
	min-width: 100%;
	padding: 15px 0;
}

